				YAHOO.util.Event.on(window, "load", startglowhandlers, { id: "xtdatatable", infotype: "player information", urlbase: "playerinfo" }); 
			</script>				       <script type='text/javascript'>
			function openfinddialog()
			{
				YAHOO.findplayerdialog.show();
			}
			
			function initfindplayerdialog() 
			{
				// Define various event handlers for Dialog
				var handlefindplayerCancel = function() {
					this.cancel();

				};
				
				// Make the dialog visible
				YAHOO.util.Dom.setStyle('findplayerdialog', 'display', 'block');
				
				// Instantiate the Dialog
				YAHOO.findplayerdialog = new YAHOO.widget.Dialog("findplayerdialog", 
																			{ width : "350px",
																			  visible : false, 
																			  context: ['xtmenus', 'tl', 'bl'],
																			  constraintoviewport : true,
																			  buttons : [ { text:"Cancel", handler:handlefindplayerCancel } ],
																			  effect:{effect:YAHOO.widget.ContainerEffect.FADE,duration:0.25},
																			  postmethod: 'form'
																			 } );
				
				var kl = new YAHOO.util.KeyListener(document, { keys: [27] },                               
		                                              { fn:YAHOO.findplayerdialog.hide, 
		                                                scope:YAHOO.findplayerdialog,
		                                                correctScope:true } ); 
				YAHOO.findplayerdialog.cfg.queueProperty("keylisteners", [ kl ]); 		
				
				YAHOO.findplayerdialog.validate = function() {
					var data = this.getData();
					
					if (data.p==0) {
						document.location = "players.php?query="+document.getElementById('findplayername').value
						//alert("Please type a player name to search for "+document.getElementById('findplayername').value);
						return false;
					} else {
						return true;
					}
				};

				// Hook up the auto-completion behavior
				var selectionevent = new YAHOO.util.CustomEvent("playerautoselected"); 
				addplayerautocomplete("findplayername", "findplayernameContainer", "findplayerid", selectionevent, false);
				
				selectionevent.subscribe(gotplayer);
				
				// Render the Dialog
				YAHOO.findplayerdialog.render();
			}
			
			function gotplayer()
			{
//				document.title = '**'+(document.getElementById('findplayerid').value)+'**';
//				return;
				if (document.getElementById('findplayerid').value != 0)
				{
					document.getElementById('foundplayer').innerHTML = document.getElementById('findplayername').value;
					YAHOO.util.Dom.setStyle('newplayerloading', 'display', 'block');
					document.findplayerform.submit();
				}
			}
		</script>

				
